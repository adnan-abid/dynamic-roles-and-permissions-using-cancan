<h1>Super Admin: Listing Roles</h1>
<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
  </tr>
  <% @roles.each do |role| %>
    <tr>
      <td>
        <%= role.name %>
      </td>
      <td>
        <%= link_to 'Show', role %>
      </td>
      <td>
        <%= link_to 'Edit', edit_role_path(role) %>
      </td>
    </tr>
  <% end %>
</table>
<br/>
