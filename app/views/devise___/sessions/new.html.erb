<div class="login-box">
  <div class="login-logo">
    <a href="../../index2.html"><b>Admin</b>LTE</a>
  </div>
  <div class="login-box-body">
    <p class="login-box-msg">Sign in to start your session</p>
   
      <%= form_for(resource, :as => resource_name, :url => session_path(resource_name), :html => { :role => 'form'}) do |f| %>
       
        <%= devise_error_messages! %>
        <div class="form-group has-feedback">
          <%= f.email_field :email, :autofocus => true, class: 'form-control' %>
        </div>
        <div class="form-group has-feedback">
          <%= f.password_field :password, class: 'form-control' %>
        </div>
        <div class="row">
          <div class="col-xs-8">
            <% if devise_mapping.rememberable? -%>
              <div class="checkbox icheck">
                <label>
                  <%= f.check_box :remember_me %> Remember me
                </label>
              </div>
            <% end -%>
          </div>
          <div class="col-xs-4">
            <%= f.submit 'Sign in', :class => 'btn btn-primary btn-block btn-flat pull-right' %>
          </div>
         
        </div>
      <% end %>
      <div class="social-auth-links text-center">
      <p>- OR -</p>
      <a href="#" class="btn btn-block btn-social btn-facebook btn-flat"><i class="fa fa-facebook"></i> Sign in using
        Facebook</a>
      <a href="#" class="btn btn-block btn-social btn-google btn-flat"><i class="fa fa-google-plus"></i> Sign in using
        Google+</a>
    </div>
    <%- if devise_mapping.recoverable? %>
      <%= link_to "I forgot my password?", new_password_path(resource_name), class: '' %><br>
    <% end -%>
    <%- if devise_mapping.registerable? %>
      <%= link_to 'Sign up', new_registration_path(resource_name), class: 'text-center'  %>
    <% end -%>
  </div>
</div>
